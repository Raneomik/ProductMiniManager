{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "dama/doctrine-test-bundle": "^5.0",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "easycorp/easyadmin-bundle": "^1.17",
        "friendsofsymfony/ckeditor-bundle": "^1.2",
        "nubs/random-name-generator": "^2.1",
        "sensio/framework-extra-bundle": "^5.2",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/console": "^4.1",
        "symfony/debug": "^4.1",
        "symfony/flex": "^1.0",
        "symfony/form": "^4.1",
        "symfony/framework-bundle": "^4.1",
        "symfony/orm-pack": "^1.0",
        "symfony/phpunit-bridge": "^4.1",
        "symfony/security-bundle": "^4.1",
        "symfony/serializer": "^4.1",
        "symfony/test-pack": "^1.0",
        "symfony/translation": "^4.1",
        "symfony/twig-bundle": "^4.1",
        "symfony/validator": "^4.1",
        "symfony/webpack-encore-bundle": "^1.0",
        "symfony/yaml": "^4.1"
    },
    "require-dev": {
        "knpuniversity/lorem-ipsum-bundle": "^1.0",
        "sensiolabs/security-checker": "^5.0",
        "symfony/browser-kit": "^4.1",
        "symfony/css-selector": "^4.1",
        "symfony/dotenv": "^4.1",
        "symfony/maker-bundle": "^1.9",
        "symfony/profiler-pack": "^1.0",
        "symfony/stopwatch": "^3.3|^4.0",
        "symfony/twig-bundle": "^3.3|^4.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "ckeditor:install %PUBLIC_DIR%/bundles/fosckeditor --no-progress-bar --clear=keep": "symfony-cmd",
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true
        }
    }
}
